{
  "name": "@my-family/monorepo",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "build:api-gateway": "tsc -p apps/api-gateway/tsconfig.app.json",
    "build:family-graph": "tsc -p apps/family-graph-service/tsconfig.app.json",
    "build:privacy": "tsc -p apps/privacy-service/tsconfig.app.json",
    "build:gamification": "tsc -p apps/gamification-service/tsconfig.app.json",
    "build:transfer": "tsc -p apps/transfer-service/tsconfig.app.json",
    "build:notification": "tsc -p apps/notification-service/tsconfig.app.json",
    "build:cards": "tsc -p apps/cards-service/tsconfig.app.json",
    "build:admin": "tsc -p apps/admin-service/tsconfig.app.json",
    "start:api-gateway": "node dist/apps/api-gateway/src/main.js",
    "start:family-graph": "node dist/apps/family-graph-service/src/main.js",
    "start:privacy": "node dist/apps/privacy-service/src/main.js",
    "start:gamification": "node dist/apps/gamification-service/src/main.js",
    "start:transfer": "node dist/apps/transfer-service/src/main.js",
    "start:notification": "node dist/apps/notification-service/src/main.js",
    "start:cards": "node dist/apps/cards-service/src/main.js",
    "start:admin": "node dist/apps/admin-service/src/main.js",
    "dev:api-gateway": "ts-node -r tsconfig-paths/register apps/api-gateway/src/main.ts",
    "dev:family-graph": "ts-node -r tsconfig-paths/register apps/family-graph-service/src/main.ts",
    "dev:privacy": "ts-node -r tsconfig-paths/register apps/privacy-service/src/main.ts",
    "dev:gamification": "ts-node -r tsconfig-paths/register apps/gamification-service/src/main.ts",
    "dev:transfer": "ts-node -r tsconfig-paths/register apps/transfer-service/src/main.ts",
    "dev:notification": "ts-node -r tsconfig-paths/register apps/notification-service/src/main.ts",
    "dev:cards": "ts-node -r tsconfig-paths/register apps/cards-service/src/main.ts",
    "dev:admin": "ts-node -r tsconfig-paths/register apps/admin-service/src/main.ts",
    "db:create": "docker exec -i my-family-postgres psql -U my_family_user -d my_family < scripts/create-schemas.sql",
    "db:seed:levels": "docker exec -i my-family-postgres psql -U my_family_user -d my_family < scripts/seed-levels.sql",
    "db:seed:holidays": "docker exec -i my-family-postgres psql -U my_family_user -d my_family < scripts/seed-holidays.sql",
    "db:seed": "npm run db:seed:levels && npm run db:seed:holidays",
    "infra:up": "docker-compose up -d",
    "infra:down": "docker-compose down",
    "test": "jest",
    "lint": "prettier --check \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^11.1.13",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.1.13",
    "@nestjs/microservices": "^11.1.13",
    "@nestjs/platform-express": "^11.1.13",
    "@nestjs/schedule": "^6.1.1",
    "@nestjs/swagger": "^11.2.6",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^11.0.0",
    "amqplib": "^0.10.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.3",
    "opossum": "^9.0.0",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.28",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.13",
    "@nx/js": "22.5.0",
    "@swc-node/register": "1.11.1",
    "@swc/core": "1.15.8",
    "@swc/helpers": "0.5.18",
    "@types/amqplib": "^0.10.8",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.3",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^30.2.0",
    "nx": "22.5.0",
    "prettier": "^2.6.2",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
